{
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "npm run prettier-production && npm test && tsdx build",
    "test": "tsdx test",
    "test-watch": "tsdx test --watchAll",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "prettier": "prettier --write \"{**/*,*}.{md,ts}\"",
    "prettier-production": "prettier --list-different \"{**/*,*}.{md,ts}\"",
    "prettier-staged": "pretty-quick --staged --pattern \"{**/*,*}.{md,ts}\"",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "name": "diff-trees",
  "keywords": [
    "diff",
    "tree",
    "trees"
  ],
  "author": "Christian Hamburger Gr√∏ngaard",
  "module": "dist/diff-trees.esm.js",
  "size-limit": [
    {
      "path": "dist/diff-trees.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/diff-trees.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.0",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "size-limit": "^4.9.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  }
}
